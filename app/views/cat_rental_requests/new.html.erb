<form action = "<%= cat_rental_requests_url %>" method = "post">
	<label for="select_cat">Which cat?</label>
	<select name="rental[cat_id]">
		<option></option>
		<% @cats.each do |cat| %>
			<option value=<%= cat.id %>><%= cat.name %></option>
			<% end %>
	</select>
	<br>
	<label for = "rental_start_date">Rental Start Date </label>
	<input name = "rental[start_date]" id = "rental_start_date" type = "date">
	<br>
	<label for = "rental_end_date">Rental End Date </label>
	<input name = "rental[end_date]" id = "rental_end_date" type = "date">
	<br>
	<input type = "submit" value="Submit">
</form>
